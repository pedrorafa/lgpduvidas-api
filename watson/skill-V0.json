    },
    {
      "type": "standard",
      "title": "Privacidade",
      "output": {
        "generic": [
          {
            "values": [
              {
                "text": "Cada dado pessoal deve ter sua proteção garantida por medidas técnicas e administrativas capazes de resguardar acessos não permitidos e casos de vazamento acidental ou proposital, além de perdas, vendas, modificações e qualquer compartilhamento indevido"
              }
            ],
            "response_type": "text",
            "selection_policy": "sequential"
          }
        ]
      },
      "parent": "node_9_1589578283762",
      "conditions": "@fundamentos:Segurança",
      "dialog_node": "node_6_1589496812733",
      "previous_sibling": "node_4_1589844624397"
    },
    {
